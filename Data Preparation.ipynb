   "source": [
    "# =========== Generate and Save Leave-One-Device-Out Indices for Training Set ===========\n",
    "\n",
    "# device_train comes from Cell 3 and has one entry per row in X_train\n",
    "device_train = np.array(device_train)\n",
    "\n",
    "# Check which devices are in the training set\n",
    "unique_devices = np.unique(device_train)\n",
    "print(\"\\nUnique devices in training set:\", unique_devices)\n",
    "print(\"Number of devices (folds):\", len(unique_devices))\n",
    "\n",
    "# Create fold assignments: one fold per device\n",
    "fold_assignments = np.zeros(len(X_train), dtype=int)\n",
    "\n",
    "for fold_num, dev in enumerate(unique_devices):\n",
    "    fold_assignments[device_train == dev] = fold_num\n",
    "\n",
    "# Sanity checks\n",
    "print(\"\\nFold ids present in fold_assignments:\", np.unique(fold_assignments))\n",
    "print(\"Total training samples:\", len(fold_assignments))\n",
    "\n",
    "# Save fold assignments as numpy array\n",
    "np.save(f\"{save_dir}/train_folds.npy\", fold_assignments)\n",
    "print(f\"\\nSaved leave-one-device-out folds as train_folds.npy in {save_dir}\\n\")\n"
   ]
